<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="MainLogPane" prefHeight="550.0" prefWidth="900.0" stylesheets="@MainLoginStyle.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainLoginController">
   <children>
      <Label layoutX="297.0" layoutY="51.0" text="医疗管理登录系统">
         <effect>
            <Reflection bottomOpacity="0.2" fraction="0.5" topOffset="-5.0" />
         </effect>
         <font>
            <Font name="System Bold" size="40.0" />
         </font>
      </Label>
      <ImageView fitHeight="172.0" fitWidth="158.0" layoutX="109.0" layoutY="166.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/doctor.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="166.0" fitWidth="158.0" layoutX="359.0" layoutY="167.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/patient.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="158.0" fitWidth="158.0" layoutX="620.0" layoutY="167.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/setting.png" />
         </image>
      </ImageView>
      <Button layoutX="154.0" layoutY="357.0" mnemonicParsing="false" onAction="#enterDocLogin" text="医生登录" />
      <Button fx:id="patLogin" layoutX="404.0" layoutY="357.0" mnemonicParsing="false" onMouseClicked="#toPatLogin" text="患者登录" />
      <Button fx:id="adminLogin" layoutX="659.0" layoutY="357.0" mnemonicParsing="false" onAction="#enterAdminLogin" text="管理员登录" />
   </children>
</AnchorPane>
